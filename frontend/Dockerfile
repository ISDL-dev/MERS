FROM node:18.12.1-alpine

WORKDIR /usr/src/app

ARG REACT_APP_ENV_TYPE
ARG REACT_APP_RATING_SECOND
ARG REACT_APP_MEDIANUM
ARG BACKEND_ENDPOINT
ENV REACT_APP_ENV_TYPE=${REACT_APP_ENV_TYPE}
ENV REACT_APP_RATING_SECOND=${REACT_APP_RATING_SECOND}
ENV REACT_APP_MEDIANUM=${REACT_APP_MEDIANUM}
ENV REACT_APP_BACKEND_ENDPOINT=${BACKEND_ENDPOINT}